import java.util.*;
class OddIndexEle {
    public static void main(String[] args) {
        Scanner in=new Scanner(System.in);
        int n=in.nextInt();
        int a[]=new int[n];
        for(int i=0;i<n;i++){
            a[i]=in.nextInt();
        }
        for(int i=0;i<n;i++){
            if(i%2 != 0){
                System.out.println(a[i]+" ");
            }
        }
    }
}
